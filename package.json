{
    "private": true,
    "scripts": {
        "generateReadme": "node    -r source-map-support/register ./dist/readme/generateReadme.js",
        "startApi": "      node    -r source-map-support/register ./dist/api/www.js",
        "watchApi": "      nodemon -r source-map-support/register ./dist/api/www.js",
        "devApi": "  cross-env NODE_ENV=development webpack --watch --config build/webpack.config.api.ts",
        "devReadme": "  cross-env NODE_ENV=development webpack --watch --config build/webpack.config.readme.ts",
        "devWeb": "  cross-env NODE_ENV=development webpack serve   --config build/webpack.config.web.ts",
        "buildApi": "cross-env NODE_ENV=production  webpack         --config build/webpack.config.api.ts",
        "buildReadme": "cross-env NODE_ENV=production  webpack         --config build/webpack.config.readme.ts",
        "buildWeb": "cross-env NODE_ENV=production  webpack         --config build/webpack.config.web.ts && cross-env NODE_ENV=production  webpack --config build/webpack.config.ssg.ts",
        "stageWeb": "cross-env NODE_ENV=development webpack         --config build/webpack.config.web.ts && cross-env NODE_ENV=development webpack --config build/webpack.config.ssg.ts",
        "dev": "  concurrently 'yarn devApi'   'yarn devReadme'   'yarn devWeb'",
        "build": "concurrently 'yarn buildApi' 'yarn buildReadme' 'yarn buildWeb'",
        "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
        "analyze": "webpack-bundle-analyzer dist/web/public/stats.json",
        "postinstall": "patch-package"
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-typescript"
        ],
        "plugins": [
            [
                "@babel/transform-runtime"
            ],
            [
                "babel-plugin-root-import",
                {
                    "paths": [
                        {
                            "rootPathSuffix": "./src/",
                            "rootPathPrefix": "@/"
                        }
                    ]
                }
            ]
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/plugin-transform-runtime": "^7.14.5",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-typescript": "^7.12.7",
        "@babel/register": "^7.13.14",
        "@nuxt/devalue": "^2.0.0",
        "@octokit/rest": "^18.6.7",
        "@types/express": "^4.17.12",
        "@types/http-errors": "^1.8.0",
        "@types/jsdom": "^16.2.10",
        "@types/lodash-es": "^4.17.6",
        "@types/mini-css-extract-plugin": "^2.0.0",
        "@types/morgan": "^1.9.2",
        "@types/node": "^14",
        "@types/pluralize": "^0.0.29",
        "@types/webpack": "^5.28.0",
        "@types/webpack-bundle-analyzer": "^4.4.1",
        "@types/webpack-manifest-plugin": "^3.0.5",
        "@types/webpack-node-externals": "^2.5.1",
        "@typescript-eslint/eslint-plugin": "^5.21.0",
        "@typescript-eslint/parser": "^5.21.0",
        "@vue/compiler-sfc": "^3.0.11",
        "@vue/server-renderer": "^3.1.1",
        "animate.css": "^4.1.1",
        "axios": "^0.27.2",
        "babel-plugin-root-import": "^6.6.0",
        "concurrently": "^7.1.0",
        "copy-webpack-plugin": "^10.2.4",
        "cross-env": "^7.0.3",
        "css-loader": "^6.1.0",
        "dayjs": "^1.10.6",
        "dotenv": "^16.0.0",
        "esbuild-loader": "^2.15.1",
        "eslint": "^8.7.0",
        "eslint-config-standard": "^17.0.0-0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-n": "^15.2.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-vue": "^8.7.1",
        "eslint-utils": "^3.0.0",
        "express": "^4.17.1",
        "highlight.js": "^11.1.0",
        "html-webpack-plugin": "^5.3.1",
        "http-errors": "^2.0.0",
        "jsdom": "^19.0.0",
        "lodash-es": "^4.17.21",
        "medium-zoom": "^1.0.6",
        "mini-css-extract-plugin": "^2.0.0",
        "morgan": "^1.10.0",
        "null-loader": "^4.0.1",
        "patch-package": "^6.4.7",
        "pluralize": "^8.0.0",
        "postinstall-postinstall": "^2.1.0",
        "puppeteer-prerender-plugin": "^3.0.10",
        "responsive-loader": "^2.3.0",
        "sass": "^1.32.8",
        "sass-loader": "^12.0.0",
        "sharp": "^0.30.4",
        "sitemap-webpack-plugin": "^1.1.1",
        "source-map-support": "^0.5.21",
        "typescript": "^4.3.2",
        "vue": "^3.1.4",
        "vue-loader": "^17.0.0",
        "vue-meta": "^3.0.0-alpha.9",
        "vue-router": "^4.0.10",
        "vue-ssr-assets-plugin": "^0.2.0",
        "webpack": "^5.28.2",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.6.0",
        "webpack-dev-server": "^4.8.1",
        "webpack-manifest-plugin": "^5.0.0",
        "webpack-node-externals": "^3.0.0"
    },
    "resolutions": {
        "@types/webpack": "^5.28.0",
        "webpack": "^5.28.2",
        "**/eslint-import-resolver-node": "0.3.4"
    }
}
