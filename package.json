{
    "private": true,
    "scripts": {
        "generateReadme": "node    -r source-map-support/register ./dist/readme/generateReadme.js",
        "startApi": "      node    -r source-map-support/register ./dist/api/www.js",
        "watchApi": "      nodemon -r source-map-support/register ./dist/api/www.js",
        "devApi": "     NODE_ENV=development webpack --watch --config build/webpack.config.api.ts",
        "devReadme": "  NODE_ENV=development webpack --watch --config build/webpack.config.readme.ts",
        "devWeb": "     NODE_ENV=development webpack serve   --config build/webpack.config.web.ts",
        "buildApi": "                        webpack         --config build/webpack.config.api.ts",
        "buildReadme": "                     webpack         --config build/webpack.config.readme.ts",
        "buildWebClient": "                  webpack         --config build/webpack.config.web.ts",
        "buildWebServer": "                  webpack         --config build/webpack.config.ssg.ts",
        "buildWeb": "yarn buildWebClient && yarn buildWebServer",
        "dev": "  concurrently 'yarn devApi'   'yarn devReadme'   'yarn devWeb'",
        "build": "concurrently 'yarn buildApi' 'yarn buildReadme' 'yarn buildWeb'",
        "lint": "vue-tsc --noemit && eslint",
        "analyze": "WEBPACK_ANALYZE=true NODE_ENV=production webpack --config build/webpack.config.web.ts",
        "prepare": "patch-package"
    },
    "devDependencies": {
        "@aws-sdk/client-s3": "^3.201.0",
        "@eslint/compat": "^1.1.0",
        "@eslint/js": "^9.4.0",
        "@nuxt/devalue": "^2.0.0",
        "@octokit/rest": "^21.0.0",
        "@stylistic/eslint-plugin": "^2.1.0",
        "@types/express": "^4.17.12",
        "@types/http-errors": "^2.0.1",
        "@types/jsdom": "^21.1.0",
        "@types/katex": "^0.16.5",
        "@types/node": "^20.4.9",
        "@types/pluralize": "^0.0.33",
        "@types/webpack": "^5.28.0",
        "@types/webpack-bundle-analyzer": "^4.4.1",
        "@types/webpack-env": "^1.18.0",
        "@unhead/ssr": "^1.8.10",
        "@unhead/vue": "^1.8.10",
        "@vue/compiler-sfc": "^3.0.11",
        "@vue/server-renderer": "^3.1.1",
        "concurrently": "^8.0.1",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^7.1.2",
        "date-fns": "^3.3.0",
        "dotenv": "^16.0.0",
        "esbuild-loader": "^4.0.0",
        "eslint": "^9.4.0",
        "eslint-plugin-n": "^17.7.0",
        "eslint-plugin-vue": "^9.9.0",
        "express": "^4.17.1",
        "gl-matrix": "^3.4.3",
        "highlight.js": "^11.8.0",
        "html-webpack-plugin": "^5.3.1",
        "http-errors": "^2.0.0",
        "jsdom": "^24.0.0",
        "katex": "^0.16.9",
        "medium-zoom": "^1.0.6",
        "mini-css-extract-plugin": "^2.0.0",
        "null-loader": "^4.0.1",
        "patch-package": "^8.0.0",
        "pluralize": "^8.0.0",
        "puppeteer-prerender-plugin": "^3.0.10",
        "responsive-loader": "^3.0.4",
        "sass": "^1.32.8",
        "sass-loader": "^14.0.0",
        "sharp": "^0.32.0",
        "sitemap-webpack-plugin": "^1.1.1",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.2",
        "typescript-eslint": "^8.0.0-alpha.24",
        "vue": "^3.1.4",
        "vue-loader": "^17.0.0",
        "vue-router": "^4.0.10",
        "vue-ssr-assets-plugin": "^0.4.0",
        "vue-tsc": "^2.0.7",
        "webpack": "^5.28.2",
