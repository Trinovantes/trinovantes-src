{
    "private": true,
    "scripts": {
        "generateReadme": "node    -r source-map-support/register ./dist/readme/generateReadme.js",
        "startApi": "      node    -r source-map-support/register ./dist/api/www.js",
        "watchApi": "      nodemon -r source-map-support/register ./dist/api/www.js",
        "devApi": "     cross-env NODE_ENV=development webpack --watch --config build/webpack.config.api.ts",
        "devReadme": "  cross-env NODE_ENV=development webpack --watch --config build/webpack.config.readme.ts",
        "devWeb": "     cross-env NODE_ENV=development webpack serve   --config build/webpack.config.web.ts",
        "buildApi": "   cross-env NODE_ENV=production  webpack         --config build/webpack.config.api.ts",
        "buildReadme": "cross-env NODE_ENV=production  webpack         --config build/webpack.config.readme.ts",
        "buildWeb": "   cross-env NODE_ENV=production  webpack         --config build/webpack.config.web.ts && cross-env NODE_ENV=production  webpack --config build/webpack.config.ssg.ts",
        "stageWeb": "   cross-env NODE_ENV=development webpack         --config build/webpack.config.web.ts && cross-env NODE_ENV=development webpack --config build/webpack.config.ssg.ts",
        "dev": "  concurrently 'yarn devApi'   'yarn devReadme'   'yarn devWeb'",
        "build": "concurrently 'yarn buildApi' 'yarn buildReadme' 'yarn buildWeb'",
        "lint": "vue-tsc --noemit && eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
        "analyze": "webpack-bundle-analyzer dist/web/public/stats.json",
        "postinstall": "patch-package"
    },
    "devDependencies": {
        "@aws-sdk/client-s3": "^3.201.0",
        "@babel/core": "^7.12.10",
        "@babel/plugin-transform-runtime": "^7.14.5",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-typescript": "^7.12.7",
        "@babel/register": "^7.13.14",
        "@nuxt/devalue": "^2.0.0",
        "@octokit/rest": "^19.0.3",
        "@types/express": "^4.17.12",
        "@types/http-errors": "^2.0.1",
        "@types/jsdom": "^21.1.0",
        "@types/morgan": "^1.9.2",
        "@types/node": "^18.15.3",
        "@types/pluralize": "^0.0.29",
        "@types/webpack": "^5.28.0",
        "@types/webpack-bundle-analyzer": "^4.4.1",
        "@types/webpack-env": "^1.18.0",
        "@types/webpack-node-externals": "^3.0.0",
        "@typescript-eslint/eslint-plugin": "^5.21.0",
        "@typescript-eslint/parser": "^5.21.0",
        "@vue/compiler-sfc": "^3.0.11",
        "@vue/server-renderer": "^3.1.1",
        "axios": "^1.0.0",
        "babel-plugin-root-import": "^6.6.0",
        "babel-plugin-transform-define": "^2.0.1",
        "concurrently": "^7.1.0",
        "copy-webpack-plugin": "^11.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.1.0",
        "dayjs": "^1.10.6",
        "dotenv": "^16.0.0",
        "esbuild-loader": "^3.0.0",
        "eslint": "^8.7.0",
        "eslint-config-standard": "^17.0.0-0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-n": "^15.2.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-vue": "^9.9.0",
